<h1 id="suh">Signup</h1>
<form class="su"  action="/signup" method="post" >
    <input type="text" name="user[first_name]" placeholder="First Name"><br><br>
    <input type="text" name="user[last_name]" placeholder="Last Name"><br><br>
    <input type="email" name="user[email]" placeholder="Email"><br><br>
    <input type="password" name="user[password]" placeholder="create a password"><br><br>
    <input type="submit" id="add-btn" value="Create Account">
   
</form>
 <%# <a href="/signin">Sign in</a> %>